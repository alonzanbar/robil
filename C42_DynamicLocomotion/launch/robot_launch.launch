<launch>

  
  <arg name="gzname" default="gazebo"/>

  <!-- start gazebo with the Atlas -->
  <include file="$(find atlas_utils)/launch/atlas_no_controllers.launch">
    <arg name="gzname" value="$(arg gzname)"/>
  </include>

  <include file="$(find atlas_utils)/launch/atlas_bringup.launch"/>



  <!-- Start init_zmp node -->
  <node pkg="C42_DynamicLocomotion" type="ZMPWalkServer.py" name="ZMPwalkServer"/>



 <!-- zmp_node  -->
  <node pkg="ZMP" type="zmp_main.py" name="zmp_node" />
  
  <!-- Leg IK caller -->
  <node pkg="leg_ik" type="IK_Caller.py" name="IK_Caller" />

  <!-- Start CoM publisher node -->
  <node pkg="hrl_kinematics" type="test_stability" name="test_stability"/>

  <!-- Leg IK server -->
  <node pkg="leg_ik" type="leg_ik_server3D.py" name="leg_ik_server" />

  
</launch>
