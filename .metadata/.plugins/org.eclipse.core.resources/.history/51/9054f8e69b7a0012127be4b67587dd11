package logger;

import java.io.OutputStream;

public class Log {

	private static void logMessage(OutputStream outputStream, String tag, String message) {
		
		StackTraceElement callingMethod = "";
		StackTraceElement[] stackTraceElements = Thread.currentThread().getStackTrace();
		if (stackTraceElements.length > 0)
			callingMethod = stackTraceElements[stackTraceElements.length - 1].getMethodName();

	}
	
	public static void d(String message) {
		
	}
}
