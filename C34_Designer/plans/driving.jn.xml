<plan descriptions="./plans/TaskDescriptions.xml">
   <par name="event1"> 
         <par name="Perception"> 
               <tsk name="LocalizationTrack" />
         </par>
         <seq name="Mission"> 
               <seq name="InCar"> 
                  <dec name="!L!"> 
                        <seq name="Setting"> 
                              <seq name="TestControllers"> 
                                    <tsk name="GripSteeringWheel(angle=1.5)" />
                                    <tsk name="CalibrateWheel" />
                                    <tsk name="PlaceFeetOnPedals(angle=1.5)" />
                                    <tsk name="CalibratePedals" />
                                    <tsk name="GripHandBrake(operation=1)" />
                                    <tsk name="GripGearStick(operation=1)" />
                              </seq>
                        </seq>
                  </dec>
                     <par name="Driving"> 
                           <tsk name="PathPlanning" />
                        <dec name="!L!"> 
                              <seq name="Driving"> 
                                    <tsk name="PathPlanningFocus(target=Gate)" />
                                    <seq name="@4"> 
                                          <tsk name="InitDrive" />
                                          <sel name="@5"> 
                                                <tsk name="WayPointsDriving" />
                                                <tsk name="FinishDrive" />
                                          </sel>
                                          <tsk name="FinishDrive" />
                                    </seq>
                              </seq>
                        </dec>
                     </par>
               </seq>
         </seq>
   </par>
</plan>