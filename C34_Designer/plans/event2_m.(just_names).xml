<plan>
   <seq name="event2"> 
         <tsk name="InitialDismountedTraversabilityMission" />
         <par name="@1"> 
               <par name="Perception"> 
                     <tsk name="LocalizationTrack" />
                     <tsk name="ObstacleDetection" />
                     <tsk name="PathPlanning" />
               </par>
               <par name="MissionProgress"> 
                     <tsk name="Monitor(target=LiveLock)" />
                  <dec name="!L!"> 
                        <seq name="Mission"> 
                              <tsk name="StandUp" />
                              <par name="@3"> 
                                    <tsk name="Monitor(target=Falling)" />
                                 <dec name="!L!"> 
                                       <seq name="SearchObjectAndGo"> 
                                             <tsk name="SearchObject(target=A)" />
                                             <par name="@5"> 
                                                   <tsk name="TrackObject(target=A)" />
                                                   <seq name="SearchPathAndGo"> 
                                                         <tsk name="PathPlanningFocus(target=A)" />
                                                         <tsk name="TurnTo(target=A)" />
                                                         <par name="OperatorCheckPoint"> 
                                                            <dec name="L"> 
                                                                  <seq name="Stabilization"> 
                                                                        <tsk name="Monitor(target=Stable)" />
                                                                        <tsk name="PostureControl(pose=stabilization)" />
                                                                  </seq>
                                                            </dec>
                                                            <dec name="T"> 
                                                                  <par name="TimeControl"> 
                                                                        <seq name="OperatorRequest"> 
                                                                              <tsk name="PushHMI(data=panoramic_image)" />
                                                                              <tsk name="PushHMI(data=occupancy_grid)" />
                                                                              <tsk name="PushHMI(data=path)" />
                                                                              <tsk name="HMIResponce" />
                                                                        </seq>
                                                                        <tsk name="Monitor(target=Timeout)" />
                                                                  </par>
                                                            </dec>
                                                         </par>
                                                         <tsk name="DirectSensorsAtNearPathArea" />
                                                         <tsk name="UpdateLocalPathToAllowThreading" />
                                                         <swi name="Move"> 
                                                               <par name="AdvanceByWalkingAlongPath"> 
                                                                     <tsk name="FollowPath" />
                                                                     <par name="Exceptions"> 
                                                                        <dec name="F:1"> 
                                                                              <tsk name="whilePlaceToThreadIn" />
                                                                        </dec>
                                                                        <dec name="F:2"> 
                                                                              <tsk name="whileNoObstacles" />
                                                                        </dec>
                                                                        <dec name="F:3"> 
                                                                              <tsk name="whilePathSolution" />
                                                                        </dec>
                                                                        <dec name="F:4"> 
                                                                              <tsk name="untilSpeedAveregeIsLowerThen(kmh=?)" />
                                                                        </dec>
                                                                        <dec name="F:4"> 
                                                                              <tsk name="untilOperatorIntervention" />
                                                                        </dec>
                                                                        <dec name="F:4"> 
                                                                              <tsk name="whileSolution" />
                                                                        </dec>
                                                                     </par>
                                                               </par>
                                                               <seq name="NoPlaceToTreadIn"> 
                                                                     <tsk name="Stop" />
                                                                     <tsk name="DirectSensorsAt270ForwardSection" />
                                                                     <tsk name="PlanDetour" />
                                                                     <par name="OperatorCheckPoint"> 
                                                                        <dec name="T"> 
                                                                              <par name="TimeControl"> 
                                                                                    <seq name="OperatorRequest"> 
                                                                                          <tsk name="PushHMI(data=panoramic_image)" />
                                                                                          <tsk name="HMIResponce" />
                                                                                    </seq>
                                                                                    <tsk name="Monitor(target=Timeout)" />
                                                                              </par>
                                                                        </dec>
                                                                        <dec name="L"> 
                                                                              <seq name="Stabilization"> 
                                                                                    <tsk name="Monitor(target=Stable)" />
                                                                                    <tsk name="PostureControl(pose=stabilization)" />
                                                                              </seq>
                                                                        </dec>
                                                                     </par>
                                                               </seq>
                                                               <seq name="Obstacle"> 
                                                                     <tsk name="AdjustPathPlan" />
                                                               </seq>
                                                               <seq name="NoPathSolution"> 
                                                                     <tsk name="DirectSensorsAt270RearSection" />
                                                                     <tsk name="GoBackAlongPath(distance=?)" />
                                                                     <tsk name="TurnTo(target=A)" />
                                                                     <tsk name="DirectSensorsAt270ForwardSection" />
                                                                     <tsk name="PlanDetour" />
                                                                     <par name="OperatorCheckPoint"> 
                                                                        <dec name="L"> 
                                                                              <seq name="Stabilization"> 
                                                                                    <tsk name="Monitor(target=Stable)" />
                                                                                    <tsk name="PostureControl(pose=stabilization)" />
                                                                              </seq>
                                                                        </dec>
                                                                        <dec name="T"> 
                                                                              <par name="TimeControl"> 
                                                                                    <seq name="OperatorRequest"> 
                                                                                          <tsk name="PushHMI(data=panoramic_image)" />
                                                                                          <tsk name="HMIResponce" />
                                                                                    </seq>
                                                                                    <tsk name="Monitor(target=Timeout)" />
                                                                              </par>
                                                                        </dec>
                                                                     </par>
                                                               </seq>
                                                               <seq name="NoSolution"> 
                                                                     <par name="OperatorControlPoint"> 
                                                                        <dec name="L"> 
                                                                              <seq name="Stabilization"> 
                                                                                    <tsk name="Monitor(target=Stable)" />
                                                                                    <tsk name="PostureControl(pose=stabilization)" />
                                                                              </seq>
                                                                        </dec>
                                                                        <dec name="T"> 
                                                                              <par name="TimeControl"> 
                                                                                    <seq name="OperatorRequest"> 
                                                                                          <tsk name="PushHMI(data=panoramic_image)" />
                                                                                          <tsk name="HMIResponce" />
                                                                                    </seq>
                                                                                    <tsk name="Monitor(target=Timeout)" />
                                                                              </par>
                                                                        </dec>
                                                                     </par>
                                                               </seq>
                                                         </swi>
                                                   </seq>
                                             </par>
                                       </seq>
                                 </dec>
                              </par>
                        </seq>
                  </dec>
               </par>
         </par>
   </seq>
</plan>